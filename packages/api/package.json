{
  "name": "@gachdong/api",
  "version": "0.1.0",

  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./auth": {
      "types": "./dist/auth/swagger.d.ts",
      "require": "./dist/auth/swagger.js",
      "import": "./dist/auth/swagger.mjs"
    },
    "./application": {
      "types": "./dist/application/swagger.d.ts",
      "require": "./dist/application/swagger.js",
      "import": "./dist/application/swagger.mjs"
    },
    "./club": {
      "types": "./dist/club/swagger.d.ts",
      "require": "./dist/club/swagger.js",
      "import": "./dist/club/swagger.mjs"
    },
    "./user": {
      "types": "./dist/user/swagger.d.ts",
      "require": "./dist/user/swagger.js",
      "import": "./dist/user/swagger.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "generate-api": "ts-node scripts/generate-api.ts",
    "generate-api:auth": "ts-node scripts/generate-api.ts auth",
    "generate-api:application": "ts-node scripts/generate-api.ts application",
    "generate-api:club": "ts-node scripts/generate-api.ts club"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "dotenv": "^16.4.5",
    "swagger-typescript-api": "^13.0.22",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5"
  }
}
